    "version": "0.2.0",
    "configurations": [
        {
            "name": "video-editor",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-browser-flag",
                "--enable-features=SharedArrayBuffer"
            ]
        },
        {
            "name": "video-editor (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "video-editor (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}